<div class="container">
    <div class="row">
      <div class="col-md-6 col-md-offset-3">
        <h2>Admin SignUp Form</h2>
        <form class="signup" action="/adminsignup" method="post">
          <div class="form-group">
            <label for="InputAccount">Account Number</label>
            <input type="account" class="form-control" id="account-input" placeholder="Account Number" name="accountNumber">
          </div>
          <div class="form-group">
            <label for="InputWebId">WebId</label>
            <input type="webid" class="form-control" id="webid-input" placeholder="WebId" name="webId">
          </div>
          <div class="form-group">
            <label for="InputPassword">Password</label>
            <input type="password" class="form-control" id="password-input" placeholder="Password" name="password">
          </div>
          <button id="admin-submit" type="submit" class="btn btn-default">Sign Up</button>
        </form>
        <br />
      </div>
    </div>
  </div>
  <br>
  <br>

   {{> footer }}